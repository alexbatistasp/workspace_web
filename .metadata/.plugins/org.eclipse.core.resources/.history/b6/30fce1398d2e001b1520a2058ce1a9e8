## parametros de conexão com o mysql
## 1 - nome do usuario do banco de dados
spring.datasource.username = novousuario
## 2 - senha do banco de dados
spring.datasource.password = password
## 3 - url da base de dados
spring.datasource.url = jdbc:mysql://localhost:3306/proj_agendamento?useTimezone=true&serverTimezone=UTC
